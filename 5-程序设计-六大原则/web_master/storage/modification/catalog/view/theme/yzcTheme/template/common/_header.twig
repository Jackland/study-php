<body class="{{ class }}">
{{ this.beginBody() }}

  {% if notification is defined and notification %}
    {{ notification }}
  {% endif %}

  <style>
    #top .container {
      padding: 0 10px;
      height: 34px;
    }

    .btn-group > .btn:first-child {
      margin-left: -5px;
    }

    .header-badge-dot {
      position: relative;
      display: inline-block;
      font-size: 12px;
      text-align: center;
      background-color: #FF5722;
      color: #fff;
      width: 6px;
      height: 6px;
      padding: 0;
      border-radius: 50%;
      top: -5px;
      margin: -7px 3px 0 1px;
    }

    .header-right-dot {
      position: relative;
      display: inline-block;
      font-size: 12px;
      text-align: center;
      background-color: #FF5722;
      color: #fff;
      width: 6px;
      height: 6px;
      padding: 0;
      border-radius: 50%;
      top: -5px;
      margin: -7px 3px 0 1px;
    }
    .registerLogin a:hover{
      color: #FA6400 !important;
      text-decoration: underline;
    }
    .nav-img-box{
      /*background-image: url("image/pantersAdv/backgroundimg.jpg");*/
      background-color: #dee4e4;
      width: 100%;
      overflow: hidden;
    }
  </style>
  {% if displayTop %}
  <nav id="top">
   {# 4521 隐藏头部广告图，这里没删除是因为未来如果还有需要，直接换图片即可  #}
    <div class="nav-img-box" style="display: none">
      <a href="https://www.ciff.furniture/" target="_blank"><img style="height: 50px" src="/image/pantersAdv/navimg.jpg" alt=""></a>
    </div>
    <div class="container">
      {% if logged %}
        {% if module_shipment_time_status %}
          <marquee style="margin-top: 8px;cursor: pointer" align="left" behavior="scroll" direction="left" height="auto" width="250" hspace="5" scrollamount="6" scrolldelay="100" onMouseOut="this.start()" onMouseOver="this.stop()">
            <a href="{{ shipmentPage }}" target="_blank" style="color: #fa6c00; font-weight: 700;">{{ shipmentTimePage.title }}</a>
          </marquee>
