    }

    .shadow_img{
      width: 100%;
      height: 100%;
      left: 0;
      position: absolute;
      top: 0;
      background: black;
      opacity: 0.6;
    }

    .title_img{
      width: 100%;
      position: absolute;
      top: 40%;
      left: 0;
      text-align: center;
    }
  </style>


  {% if logged and display_common_ticket %}
    {{ ticketBuyer }}
  {% endif %}

  {% if display_menu %}
    {{ menu }}
  {% endif %}
