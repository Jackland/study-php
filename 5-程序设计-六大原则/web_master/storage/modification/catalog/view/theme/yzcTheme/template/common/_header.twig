  {% endif %}
  <header>
    <div class="container">
      <div class="row">
        <div class="col-sm-3">
          <div id="logo">
            {#logo是变量#}
            {% if logo %}
              <a href="{{ home }}" data-cnzz-event="页面头部|Hd_Logo">
                <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" style="width: 204px" class="img-responsive" />
              </a>
            {% else %}
              <h1><a href="{{ home }}">{{ name }}</a></h1>
            {% endif %}
          </div>
        </div>
        {% if display_search %}
        <div class="col-sm-4">{{ search }}</div>
        {% endif %}
        {% if display_account_info %}
        <div class="col-sm-5">
          <div class="header-right">
            {#个人账户#}
            <div class="dropdown account-wrapper " {% if logged %}style="position: relative; top: -4px;"{% endif %}>
              <div title="{{ text_account }}" class="dropdown-toggle open" data-toggle="dropdown" style="cursor: pointer" data-cnzz-event="页面头部|Hd_MyAccount">
                {#<img src="{{ base }}/image/catalog/user/AccountBlue.png" style="width: 26px; height: 26px" class="img-responsive" />#}
                <i class="giga icon-account-blue"></i>
                <div class="name-group">
                  {% if logged %}
                    <span class="name hidden-xs hidden-sm hidden-md">{{ userName }}</span>
                    <span class="customer-number">{{ userNumber }}</span>
                  {% else %}
                    <span class="name hidden-xs hidden-sm hidden-md">{{ text_account_no_login }}</span>
                  {% endif %}
                </div>
                <span class="caret"></span>
              </div>
              <div class="header-pop-up">
                <div class="placeholder-div"></div>
                {% if not logged %}
                  {#未登录#}
                  <div class="top-register-login">
                    <a href="{{ login }}" class="left" data-cnzz-event="页面头部|Hd_MyAccount_Login">{{ text_login }}</a>
                    <a href="{{ register }}" class="right" data-cnzz-event="页面头部|Hd_MyAccount_Register">{{ text_register }}</a>
                  </div>
                  <div class="header-pop-up-list">
                    <ul>
                      <li><a href="{{ account }}">{{ text_account }}</a></li>
                      <li><a href="{{ forgotten }}">{{ text_forgotten }}</a></li>
                    </ul>
                  </div>
                  {#未登录 end#}
                {% else %}
                  {#已登录#}
                  <div class="header-pop-up-list">
                    <ul id="menu_ul">
                      {% if is_seller == 0 %}
                        <li><a href="{{ account }}">{{ text_account }}</a></li>
                        <li><a href="{{ order }}">{{ text_order }}</a></li>
                        {% if brand_show == 1 %}
                          <li><a href="{{ buyer_brand_management }}">{{ text_Buyer_Brand_Management }}</a></li>
                        {% endif %}
                        <li><a href="{{ show_customer_order }}">{{ text_customer_order }}</a></li>
                        <li><a href="{{ sellers }}">{{ text_sellers }}</a></li>
                        {#{% if quote_enable %}#}
                        <li>
                          <a href="{{ product_quote_my }}">{{ text_product_quote_my }}
                            &nbsp;<span id="umenu-bid-mark-count" class="umenu-bid-mark-count" {% if bid_mark_count == '' %}style="display: none;"{% endif %} >{{ bid_mark_count }}</span>
                          </a>
                        </li>
                        {#{% endif %}#}

                        <li><a href="{{ sales_purchase_bill_url }}">{{ text_billing_management }}</a></li>
                        {#{% if isJapan %}#}
                        <li>
                          <a href="{{ rma_management }}">
                            {{ text_rma_management }}
                            &nbsp;<span
                              id="umenu-rma-mark-count"
                              class="umenu-bid-mark-count"
                              {% if unresolved_rma_count == '' %}style="display: none;"{% endif %}>
                              {{ unresolved_rma_count }}
                            </span>
                          </a>
                        </li>
                        {#{% endif %}#}
                        {#<li><a href="{{ rma_management }}">Notification</a></li>#}

                      {% else %}
                        <li style="color: #1b3564;font-weight: bold"><a href="{{ url_seller_backend }}"><i class="iconfont icon-shangdian"></i> Back To Seller Backend</a></li>
                        <hr style="margin-top: 0;margin-bottom: 0">
                        {% if marketplace_seller_mode == 0 %}
                          <li><a href="{{ show_customer_order }}">{{ text_customer_order }}</a></li>
                        {% endif %}
                        <li><a href="{{ inbound_management }}">{{ text_inbound_management }}</a></li>
                        <li><a href="{{ print_product_labels }}">{{ text_print_product_labels }}</a></li>
                        {#END if is_seller == 0 #}
                      {% endif %}


                      {% if not isInnerSeller %}
                        <li><a href="{{ inventory_management }}">{{ text_inventory_management }}</a></li>
                      {% endif %}
                      {% if is_trusteeship %}
                        <li><a href="{{ item_code_mapping }}">{{ text_item_code_mapping }}</a></li>
                      {% endif %}

                      {% if isShowMappingManagement==1 %}
                        <li><a href="{{ mappingManagementLink }}">{{ text_mapping_management }}</a></li>
                      {% endif %}

                      {% if isShowStorageFeeManagement == 1 %}
                        <li><a href="{{ storageFeeManagementLink }}">{{ text_storage_fee_management }}</a></li>
                      {% endif %}

                      <li><a href="{{ logout }}">{{ text_logout }}</a></li>
                    </ul>
                  </div>
                {% endif %}
              </div>
            </div>
            {#收藏夹#}
            <div class="wishlist">
              {% if text_wishlist_num >= 0 %}
                <span class="icon-sum" id="wishlist-total-num">{{ text_wishlist_num }}</span>
              {% endif %}
              <a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}" data-cnzz-event="页面头部|Hd_SavedItems">
                {#<img src="{{ base }}/image/catalog/cart/HeartBlue.png" style="width: 26px; height: 26px;" class="img-responsive" />#}
                <i class="giga icon-heart"></i>
                <span class="hidden-xs hidden-sm hidden-md">{{ text_wishlist }}</span>
              </a>
              {% if not logged %}
                <div class="header-pop-up">
                  <div class="placeholder-div"></div>
                  <div class="top-register-login">
                    <a href="{{ login }}" class="left" data-cnzz-event="页面头部|Hd_SavedItem_Login">{{ text_login }}</a>
                    <a href="{{ register }}" class="right" data-cnzz-event="页面头部|Hd_SavedItem_Register">{{ text_register }}</a>
                  </div>
                  <div class="bottom-link">
                    <p>Please login or register first.</p>
                  </div>
                </div>
              {% endif %}
            </div>
            {#购物车#}
            {{ cart }}
          </div>
        </div>
        {% endif %}
        {% if display_register_header %}
          <div class="col-sm-4" style="float: right;line-height: 48px">
            Already have an account? <a href="/index.php?route=account/login" style="color: #3A75DC">Login <i class="gcl gclarrLeft" style="font-size: 12px"></i></a>
          </div>
        {% endif %}
        {% if display_forgot_password_header %}
          <div class="col-sm-4 registerLogin" style="float: right;text-align: right" >
            <a href="/index.php?route=account/register_apply" style="color: #666;line-height: 48px;padding: 0 5px">Register</a>
            <span style="color: #666">|</span>
            <a href="/index.php?route=account/login" style="color: #666;line-height: 48px;padding: 0 5px">Login</a>
          </div>
        {% endif %}
      </div>
    </div>
  </header>
  <script>
    window.addEventListener('load', function() {
      var winWidth = $(window).innerWidth();
      var imgWidth = $(".nav-img-box img").width();
      if (winWidth > 1200){
        var left = (Number(winWidth)-Number(imgWidth))/2;
      }else{
        var left = (1200-Number(imgWidth))/2;
      }
      $(".nav-img-box img").css('margin-left',left)

      window.addEventListener('resize', function() {
        var winWidth = $(window).innerWidth();
        if (winWidth > 1200){
          var left = (Number(winWidth)-Number(imgWidth))/2;
        }else{
          var left = (1200-Number(imgWidth))/2;
        }
        $(".nav-img-box img").css('margin-left',left)

      })

    })

  </script>
  <script>
      var ele_lis = document.getElementById("menu_ul");
      if(ele_lis && ele_lis.hasOwnProperty('children')){
          var lis = ele_lis.children;
          for (var i = 0; i < lis.length; i++) {
              lis[i].onmouseover = function () {
                  this.className = "current";
                  $('#second_link').css('top', this.offsetTop - 5);
              };
              lis[i].onmouseout = function () {
                  this.className = ""
              }
          }
      }
      $('.open').on('click',function(){
        let is_seller = "{{ is_seller }}" ;
        if(is_seller == 0){
          window.location.href="index.php?route=account/guide";
        }
      })
  </script>
  <style>
    #second_link {
      display: none;
      position: absolute;
      left: 254px;
      top: -8px;
      width: 200px;
      line-height: 26px;
      z-index: -1;
      border-radius: 5px;
    }

    #first_link{
      position: relative;
    }

    #first_link:hover ul {
