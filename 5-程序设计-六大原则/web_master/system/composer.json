{
  "name": "opencart/opencart",
  "type": "project",
  "description": "OpenCart",
  "keywords": [
    "opencart",
    "ecommerce",
    "framework",
    "opensource"
  ],
  "homepage": "http://www.opencart.com",
  "license": "GPL-3.0+",
  "config": {
    "vendor-dir": "./../storage/vendor/",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=7.1, <7.2",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "barcodegen/barcodegen": "^5.2",
    "doctrine/dbal": "^2.9",
    "dompdf/dompdf": "^1.0",
    "filp/whoops": "^2.7",
    "gregwar/captcha": "^1.1",
    "guzzlehttp/guzzle": "^6.5",
    "illuminate/config": "^5.8",
    "illuminate/console": "^5.8",
    "illuminate/container": "^5.8",
    "illuminate/database": "^5.8",
    "illuminate/events": "^5.8",
    "illuminate/filesystem": "^5.8",
    "illuminate/pagination": "^5.8",
    "illuminate/support": "^5.8",
    "illuminate/validation": "^5.8",
    "kaysonwu/flysystem-aliyun-oss": "^1.0",
    "kriss/bcmath": "^2.0",
    "league/flysystem": "^1.0",
    "maennchen/zipstream-php": "^2.1",
    "maximebf/debugbar": "^1.16",
    "monolog/monolog": "^1.25",
    "mpdf/mpdf": "^8.0",
    "overtrue/easy-sms": "^1.3",
    "phpoffice/phpspreadsheet": "^1.12",
    "picqer/php-barcode-generator": "^0.3.0",
    "predis/predis": "^1.1",
    "psr/log": "^1.1",
    "sofa/eloquence": "^6.0",
    "symfony/cache": "^4.4",
    "symfony/console": "^4.4",
    "symfony/http-client": "^4.4",
    "symfony/http-foundation": "^4.4",
    "symfony/lock": "^4.4",
    "symfony/var-exporter": "^4.4",
    "tecnickcom/tcpdf": "^6.2"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6"
  },
  "suggest": {
    "friendsofphp/php-cs-fixer": "For Fix: composer global require friendsofphp/php-cs-fixer"
  },
  "autoload": {
    "files": [
      "./Framework/helpers.php",
      "../src/helpers.php"
    ],
    "psr-4": {
      "Catalog\\": "./../catalog",
      "Yzc\\": "./library/yzc",
      "Framework\\": "./Framework",
      "App\\": "../src"
    }
  },
  "scripts": {
    "fix": "php-cs-fixer fix --config .php_cs"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
