{{ header }}{{ separate_column_left }}
<script type="text/javascript" src="catalog/view/javascript/layer/layer.js"></script>
<link type="text/css" href="admin/view/stylesheet/stylesheet_giga.css?v={{ app_version }}" rel="stylesheet" media="screen"/>
<title>Shipment Notice</title>
<style>
  .layer-panel-box {
    color: #000;
    font-size: 14px;
  }

  .nav-tabs {
    border-bottom: 0 solid white;
  }

  .nav-tabs-sty {
    margin-bottom: 0;
  }

  .tab-content-sty {
    background-color: white;
  }
</style>
<div class="container-fluid giga-content" id="content" style="margin-left: 15%">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
      <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
  <div class="row">
    <div class="col-md-12">
      <h1>Sales Order Upload</h1>
      <span style="float: right;margin-top: -50px;cursor: pointer;margin-right: 20px;">
          <a href="{{ url_back }}" data-toggle="tooltip" title="" class="btn btn-default pull-right" data-original-title="Back">
            <i class="fa fa-reply"></i>
          </a>
      </span>
      <div id="box-shadow">
        <div class="tab-content tab-content-sty">
          <div class="tab-pane active">
            <div class="tab-pane active" id="tab-upload">
              <fieldset>
                <h1>About Shipment Notice</h1>
                {{ description |replace({'&nbsp;': ' '}) }}

              </fieldset>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{{ footer }}

