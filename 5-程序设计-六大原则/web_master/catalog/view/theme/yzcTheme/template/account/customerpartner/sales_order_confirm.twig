<div  style="text-align: center;">
  {% if salesOrderIsLTLCheck %}
    <button type="button" onclick="getLTLList()" class="btn btn-primary"  style="color: #fff;background-color: #3975db;" data-dismiss="modal" id="btnClose">Check</button>
  {% else %}
    <button type="button" onclick="getList()" class="btn btn-primary"  style="color: #fff;background-color: #3975db;" data-dismiss="modal" id="btnClose">Check</button>
  {% endif %}
</div>

<script>
  {% if salesOrderIsLTLCheck %}
  function getLTLList(){
    var index = parent.layer.getFrameIndex(window.name);
    parent.layer.close(index);
    var next_url  = 'index.php?route=account/customerpartner/sales_order_management/getSalesOrderLtlMore&runId={{ runId }}';
    window.parent.getSalesOrderLTLMoreModel(next_url);
  }
  {% endif %}


  function getList() {
    var index = parent.layer.getFrameIndex(window.name);
    parent.layer.close(index);
    var next_url  = 'index.php?route=account/customerpartner/sales_order_list';
    window.parent.getSalesOrderList(next_url);
  }
</script>