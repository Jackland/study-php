                        <td><span class="text-price">{{ restOrder.service_fee_per }}</span></td>
                        {% endif %}
                        <td><span class="text-price">{{ restOrder.freight }}</span></td>
                        <td>
                          {% if restOrder.discount_total > 0 %}
                            <span class="text-price">{{ restOrder.final_total }}</span>
                            <br/>
                            <span class="text-price underline-price">{{ restOrder.total }}</span>
                          {% else %}
                            <span class="text-price">{{ restOrder.total }}</span>
                          {% endif %}
                        </td>
