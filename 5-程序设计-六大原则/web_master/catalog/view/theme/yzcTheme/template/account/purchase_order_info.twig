                        {% endif %}
                      </td>
                    </tr>
                  {% endfor %}
                  {#Sub-Total#}
                  <tr>
                    <td class="text-right" colspan="{{ colspan+1 }}">
                      {#{% if isEuropean %} <i class="giga icon-vat"></i> {% endif %}#}
                      Item(s) Subtotal:
                    </td>
                    <td class="text-right"><span class="text-price">{{ sub_total }}</span> </td>
                  </tr>
                  {#Service fee#}
                  {% if isEuropean %}
                    <tr>
                      <td class="text-right border-none" colspan="{{ colspan+1 }}"> Total Service Fee:</td>
                      <td class="text-right border-none"><span class="text-price">{{ total_service_fee }}</span> </td>
                    </tr>
                  {% endif %}
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}"> Total Fulfillment:</td>
                    <td class="text-right border-none"><span class="text-price">{{ total_freight }}</span> </td>
                  </tr>
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}">Order Total:</td>
                    <td class="text-right border-none"><span class="text-price">{{ total_price }}</span></td>
                  </tr>
                  {# 1452 优惠券 #}
                  {% if promotion_discount > 0 %}
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}"> Promotion Discount:</td>
                    <td class="text-right border-none"><span class="text-price">{{ promotion_discount_show }}</span> </td>
                  </tr>
                  {% endif %}
                  {% if giga_coupon > 0 %}
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}"> GIGA Coupon:</td>
                    <td class="text-right border-none"><span class="text-price">{{ giga_coupon_show }}</span> </td>
                  </tr>
                  {% endif %}
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}"> Final Total:</td>
                    <td class="text-right border-none"><span style="color:#DB0101">{{ final_total_show }}</span> </td>
                  </tr>
                  {# 1452 优惠券 End #}
                  <tr>
                    <td class="text-right border-none" colspan="{{ colspan+1 }}"> Transaction Fee:</td>
                    <td class="text-right border-none"><span class="text-price">{{ total_transaction }}</span> </td>
                  </tr>
                  {# 1452 优惠券 #}
                  {% if order_full_delivery_campaigns %}
                    <tr class="order-campaign">
                      <td colspan="{{colspan+2}}" class="text-right border-none">
                        <div class="gift-line">Gift:
                        {% for key,order_campaign in order_full_delivery_campaigns %}
                          <span class="text-price coupon-account coupon-des">{{ order_campaign.content }}</span>
                          {% if order_campaign.is_coupon %}
                          <span class="coupon-des">coupon&nbsp;</span><img src="image/coupon/coupon.png">
                          {% endif %}
                          {% if key < order_full_delivery_campaigns|length-1 %}
                          </div><div class="gift-line">
                          {% endif %}
                        {% endfor %}
                        </div>
                      </td>
                    </tr>
