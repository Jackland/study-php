                            <span class="my-discount">Discount:<span class="percent-dis"> {{ product['discountShow'] }}% </span>OFF</span>
                          {% endif %}
                        </td>
                      {% endif %}
                      <td class="text-center">
                        <div id="freight">
                          <div>{{ product.freight_per }}<span class="caret"></span></div>
                          <ul class="dropdown-freight" style="width: max-content">
                            <li style="list-style: none">
                              <div class="drop-hover-title border-bottom-gray">
                                {% if isCollectionFromDomicile %}
                                  Fulfillment for pickup service
                                {% elseif (delivery_type==2) %}
                                  Fulfillment for Cloud Wholesale Fulfillment
                                {% else %}
                                  Fulfillment for drop shipping service
                                {% endif %}
                              </div>
                              <div class="drop-hover-items">
                                <p>
                                  <span class="drop-hover-items-label">{{ base_freight_label }} :</span>
                                  <span class="drop-hover-items-num">{{ product.freight }}</span>
                                  {% if delivery_type == 2 and product.freight_difference_per >0 %}
                                  <span style="color: #FA6400">+ {{ product.freight_difference_per_str }}
                                    <i class="glyphicon glyphicon-info-sign" data-toggle="tooltip" style="cursor: pointer;font-size: 12px" title="{{ product.tips_freight_difference_per }}"></i>
                                  </span>
                                  {% endif %}
                                </p>
                                {# 超重附加费 #}
                                {% if product.overweight_surcharge > 0 %}
                                  <p style="margin-bottom: 0">
                                    <span class="drop-hover-items-label">{{ overweight_surcharge_label }} :</span>
                                    <span class="drop-hover-items-num">{{ product.overweight_surcharge_show }}</span>
                                  </p>
                                {% endif %}

                                <p style="margin-bottom: 0">
                                  <span class="drop-hover-items-label">{{ package_fee_label }} :</span>
                                  <span class="drop-hover-items-num">{{ product.package_fee }}</span>
                                </p>
                              </div>

                            </li>
