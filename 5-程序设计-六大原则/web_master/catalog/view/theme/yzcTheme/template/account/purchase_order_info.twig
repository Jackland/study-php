                            {{ global_frontend_futures_second_sign | dprintf({'id':product.agreement_id,'s_id':product.agreement_no }) }}
                          {% else %}
                            {{ global_frontend_futures_sign | dprintf({'id':product.agreement_id,'s_id':product.agreement_no }) }}
                          {% endif %}
                        {% endif %}
                        {% if product.is_rebate %}
                          <a target="_blank" href="{{ product.url_rebate }}">
                            <img data-toggle="tooltip" style="padding-left: 1px;position: relative;top: -2px;" src="{{ rebate_logo }}" data-original-title="{{ product.tip_rebate }}">
                          </a>
                        {% endif %}
                        {% if product.is_margin %}
                          <a href="{{ product.url_margin }}">
                            <span class="flag-margin-order"><span class="giga icon-margin" data-toggle="tooltip" title="{{product.tip_margin}}"></span></span>
                          </a>
                        {% endif %}
                        {{ product.sku }}
