                  <table class="table main table-hover" style="margin: 0 auto;">
                    <thead>
                    <tr>
                      <th class="text-center">{{ column_item_code }}</th>
                      <th class="text-center">{{ column_purchase_order }}</th>
                      <th class="text-center">User Code</th>
                      <th class="text-center">{{ column_purchase_date }}</th>
                      <th class="text-center">{{ column_purchase_qty }}</th>
                      <th class="text-center">Unit Price After Discount</th>
                      {% if isEuropean %}
                        <th class="text-center"style="width:12%"> Service Fee After Discount</th>
                      {% endif %}
                      <th class="text-center">Fulfillment Per Unit</th>
                      <th class="text-center">Total</th>
                      <th class="text-center"> Sales Order</th>
                      <th class="text-center">{{ column_rma_id }}</th>
                      <th class="text-center">Action</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for restOrder in restOrderList %}
                      <tr class="text-center">
                        <td>{{ restOrder.sku }}</td>
                        <td>
                          {% if restOrder.rest_order_url %}
                            <a href="{{ restOrder.rest_order_url }}" style="color: blue;font-weight: bold">{{ restOrder.rest_order_id }}&nbsp;<i class="giga icon-table-link"></i></a>
                          {% else %}
                            {{ restOrder.rest_order_id }}
                          {% endif %}
                        </td>
                        <td>{{ restOrder.buyer_user_number }}</td>
                        <td>{{ restOrder.purchase_date }}</td>
                        <td><span class="text-price">{{ restOrder.quantity }}</span></td>
                        <td>
