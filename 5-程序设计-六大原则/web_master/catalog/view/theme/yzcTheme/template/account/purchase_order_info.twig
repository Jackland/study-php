<script>
  function showImg(e){
    var newsrc=getObjectURL(e.files[0]);
    var id = e.id;
    var pattern = new RegExp("[0-9]+");
    var imgRow = id.match(pattern);
    var reader = new FileReader();
    reader.onload = function(theFile) {
      var image = new Image();
      image.onload = function() {
        $('#img'+imgRow).attr("src",newsrc);
      };
      image.src = theFile.target.result;
    }
    reader.readAsDataURL(e.files[0]);
  }
  function getObjectURL(file) {
    var url = null ;
    if (window.createObjectURL!=undefined) {
      url = window.createObjectURL(file) ;
    } else if (window.URL!=undefined) {
      url = window.URL.createObjectURL(file) ;
    } else if (window.webkitURL!=undefined) {
      url = window.webkitURL.createObjectURL(file) ;
    }
    return url ;
  }
  function showReview(orderId,orderProductId,productId,sellerId) {
    // $('#input-name').val();
    $.ajax({
      url : "index.php?route=account/order/reviewInfo",
      async : true,
      type : "POST",
      data : {
        "orderId" : orderId,
        "orderProductId" : orderProductId,
        "productId": productId
      },
      success : function(json){
        if(json['error']){
          window.location.href=json['url'];
        }else {
          //清空
          $('input[name="name"]').val('');
          $('textarea[name="text"]').val('');
          $('input[name="rating"]:checked').prop('checked', false);
          $('input[name="seller_rating"]:checked').prop('checked', false);
          $('#product_id').val('');
          $('#order_id').val('');
          $('#order_product_id').val('');
          $('#seller_id').val('');
          $('#review_id').val('');
          for (var i = 0; i < 10; i++) {
            document.querySelector('#imageAdd' + i).value = null;
            document.querySelector('#img' + i).src = "catalog/view/theme/yzcTheme/stylesheet/diyUpload/images/imageUpload.png";
          }
          if (json['add']) {
            $('#input-name').val(json['author']);
            $('#product_id').val(json['product_id']);
            $('#order_id').val(json['order_id']);
            $('#order_product_id').val(json['order_product_id']);
            $('#seller_id').val(sellerId);
          } else if (json['edit']) {
            $('#review_id').val(json['review_id']);
            $('#input-name').val(json['author']);
            $('#input-review').val(json['text']);
            $('#rating' + json['rating']).prop('checked', true);
            $('#seller_rating' + json['seller_rating']).prop('checked', true);
            $('#product_id').val(json['product_id']);
            $('#order_id').val(json['order_id']);
            $('#order_product_id').val(json['order_product_id']);
            $('#seller_id').val(sellerId);
            $('#buyer_review_number').val(json['buyer_review_number']);
            $('#seller_review_number').val(json['seller_review_number']);
            for (var i = 0; i < 10; i++) {
              // document.querySelector('#imageAdd'+i).value = null;
              if (json['img' + i] != null || json['img' + i] != undefined) {
                document.querySelector('#img' + i).src = json['img' + i];
              }
            }
          }
          $('#myModal-review').modal('show');
        }
      },
      error : function() {
        alert("请求失败");
      },
      dataType : "json"
    });
    // $('#myModal-review').modal('show');
  }
  $('#button-review').on('click', function() {
    $.ajax({
      {#url: 'index.php?route=product/product/write&product_id={{ product_id }}',#}
      url: 'index.php?route=account/order/write',
      type: 'post',
      dataType: 'json',
      data: new FormData($('#form-review')[0]),
      cache: false,
      contentType: false,
      processData: false,
      beforeSend: function () {
        $('#button-review').button('loading');
      },
      complete: function () {
        $('#button-review').button('reset');
      },
      success: function (json) {
        $('.alert-dismissible').remove();

        if (json['error']) {
          $('#review').after('<div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> ' + json['error'] + '</div>');
        }

        if (json['success']) {
          // $('#review').after('<div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> ' + json['success'] + '</div>');

          $('input[name=\'name\']').val('');
          $('textarea[name=\'text\']').val('');
          $('input[name=\'rating\']:checked').prop('checked', false);
          $('input[name=\'seller_rating\']:checked').prop('checked', false);
          $('#product_id').val('');
          $('#order_id').val('');
          $('#order_product_id').val('');
          $('#seller_id').val('');
          $('#review_id').val('');
          for(var i=0;i<10;i++){
            document.querySelector('#imageAdd'+i).value = null;
            document.querySelector('#img'+i).src = "catalog/view/theme/yzcTheme/stylesheet/diyUpload/images/imageUpload.png";
          }
          alert(json['success'])
          $('#myModal-review').modal('hide');

        }
      }
    });
  });

  function reorderCartAdd(product_id, quantity, transaction_type=undefined,delivery_type = undefined){
    let sendData = {};
    sendData.product_id = product_id;
    sendData.quantity = quantity;
    sendData.route = 'reorder';
    if (transaction_type != undefined || transaction_type != '' || transaction_type != '0' || transaction_type != 0) {
      sendData.transaction_type = transaction_type;
    }
    if (delivery_type != undefined || delivery_type != '' || delivery_type != '0' || delivery_type != 0) {
      if(delivery_type == 2) {
        sendData.freight_radio = 'cwf';
      }
    }

    $.ajax({
      url: 'index.php?route=checkout/cart/add',
      type: 'post',
      data: sendData,
      dataType: 'json',
      success: function(json) {
        $('.alert-dismissible, .text-danger').remove();
        $('.form-group').removeClass('has-error');

        if (json['error']) {
          // $('#content').before('<div class="alert alert-danger alert-dismissible container"><i class="fa fa-times-circle"></i> Failed <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
          if(json['error']['transaction_type']){
            layer.alert(json['error']['transaction_type'], {
              title: 'Message',
              btn : 'OK',
              skin: 'yzc_layer' //样式类名
              ,closeBtn: 0
            });
          }

          if (json['error']['recurring']) {
            layer.alert(json['error']['recurring'], {
              title: 'Message',
              btn : 'OK',
              skin: 'yzc_layer' //样式类名
              ,closeBtn: 0
            });
          }
          if (json['error']['msg']) {
            layer.msg(json['error']['msg']);
          }
        }

        if (json['success']) {
          $('#content').before('<div class="alert alert-success alert-dismissible container"><i class="fa fa-check-circle"></i> ' + json['success'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
          $('html, body').animate({scrollTop: 0}, 'slow');
          $('#cart').load('index.php?route=common/cart/info');
        }
      },
      error: function(xhr, ajaxOptions, thrownError) {
        layer.alert(thrownError + "<br>" + xhr.statusText + "<br>" + xhr.responseText, {
          title: 'Message',
          btn: 'OK',
          skin: 'yzc_layer' //样式类名
          , closeBtn: 0
        });
      },
      complete:function(){
      }
    });
  }
</script>
