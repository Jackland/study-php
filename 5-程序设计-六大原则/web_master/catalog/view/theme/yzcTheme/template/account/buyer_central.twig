        <p class="score-detail-total-title">Buyer Credibility</p>
        <p>
          <span class="score-detail-item">{{ comprehensiveInfo.7.score }}</span>
          <span class="score-detail-rule">(Full Score {{ comprehensiveInfo.7.full_score }})</span>
        </p>
        <p>
          {% if comprehensiveInfo.7.compare==2 %}
            <i class="giga icon-xiangshangjiantou-01 score-green"></i>
            <span class="score-item-flag">Above Average</span>
          {% elseif comprehensiveInfo.7.compare==1 %}
            <img class="align-inherit" src="/image/icons/weibian.png" alt="">
            <span class="score-item-flag">Equal to Average</span>
          {% elseif comprehensiveInfo.7.compare==0 %}
            <i class="giga icon-xiangxiajiantou-01 score-red"></i>
            <span class="score-item-flag">Below Average</span>
          {% endif %}
        </p>
      </div>
      {#   等号 #}
      <div>
        <p><img class="score-item-tab-score-img" src="image/equ.jpg"></p>
      </div>
      <div>
        <p><span class="score-equ-total">{{ comprehensive_total }}</span></p>
      </div>
    </div>
    <div class="how-to text-right">
      <a href="{{ help_center_url }}" target="_blank" class="link hot-to-txt">
        {{ __('如何提高你的GIGA Index&nbsp;&nbsp;&nbsp;&nbsp;查看详情', {}, 'catalog/view/customerpartner/seller_center/index') }}
      </a>
      <i class="giga icon-V10_Sellerhoutai_youjiantou go-how-to"></i>
    </div>
  </div>
</div>
<script>
  var FONT_COUNTRY_CURRENCY = {"DEU":"giga icon-ouyuanzhifu-01","JPN":"giga icon-riyuanzhifu-01","GBR":"giga icon-yingbangzhifu-01","USA":"giga icon-meiyuanzhifu-01"};
  var FONT_CONFIG = {
      "ToBePaid":{"fontColor":"#E00019","fontClass":"","fontBgColor":"#FAE0E0","bgColor":"#FFF5F5"},
      "Margin":{"fontColor":"#0CBA1A","fontClass":"giga icon-Margin-01","fontBgColor":"#0CBA1A1A","bgColor":"#EFFCED"},
      "Rebates":{"fontColor":"#AD3EFF","fontClass":"giga icon-rebates-01","fontBgColor":"#ad3eff1a","bgColor":"#F5F5FF"},
      "Future":{"fontColor":"#2861CE","fontClass":"giga icon-future-01","fontBgColor":"#2861CE1A","bgColor":"#F0F6FF"},
      "Spot":{"fontColor":"#FF8900","fontClass":"giga icon-jietiticheng","fontBgColor":"#FF89001A","bgColor":"#FFF7ED"}
  };
  var country = "{{ country|default('USA') }}";
  var objCollectionWeek = {{ objCollectionWeek|default('{}') }};
  var listCollectionMonth = {{ listCollectionMonth|default('[]') }};
  var currentMonth = objCollectionWeek.month;
  var currentYear = objCollectionWeek.year;
  var currentYmd = '';
  var clickType = 1; // 类型
  var clickDay = ''; // 天
  var clickMonth = objCollectionWeek.month; // 月
  var lastExactDate = ''; // 当前月的day
  var ymd = ''; // 年-月-日 的key值
  var numPending = {{ buyer_sale_order_and_bid_statistics|length|default(0) }};

  var currentMonthObject = null;
  var nextMonthObject = null;

  // 初始化数据
  $(function () {
      FONT_CONFIG.ToBePaid.fontClass = FONT_COUNTRY_CURRENCY[country];
      if(numPending<5){
          $(".num").css("width","calc(25% - 1px)")
      }else{
          $(".num").css("width","calc(20% - 1px)")
      }
    $("#newTime1").text(objCollectionWeek.monthEnglish+' '+objCollectionWeek.year);
    loadMonth(0,'',currentMonth,'out');
  });

  // viewMore按钮
  $("#viewMore").on('click',function () {
      LoadMonthAfterGetEvent();
    $("#myModalMonthEvent").modal('show');
    loadMonth(clickType,clickDay,clickMonth,'inner');
    // 加载右侧列表
    $("#exactDate").html(lastExactDate);
    disabledBtn(clickType);
    // 渲染事件
    let list = getListEventByKey(ymd);
    loadEventList(list);
  });

  // 日历点击事件
  $("body").on('click','.badge-box-td',function () {
    let day = $(this).find('span').attr('data-day');
    let month = $(this).find('span').attr('data-month');
    let type = $(this).find('span').attr('data-type');
    let typeStr = $(this).find('span').attr('data-type-str');
    let exactDate = $(this).find('span').attr('data-date');
    let ymd1 = $(this).find('span').attr('data-ymd');

    // 判断type是否是14天的日期
    if(type==0){
        LoadMonthAfterGetEvent();
        ymd = ymd1;
        if(ymd!=currentYmd){
            $("body #newCalendar .badge-box").removeClass('badge-active');
            $(this).find('span').addClass('badge-active');
        }else{
            $("body #newCalendar .badge-box").removeClass('badge-active');
        }
      $("#myModalMonthEvent").modal('show');
      // 判断当前点击的日期月份，不是当前月份是，type为2代表下一月
      if(currentMonth!=month){
        type = 2;
      }else{
        type = 1;
      }
      clickType = type;
      loadMonth(type,day,month,'inner');
      disabledBtn(type);
    }else{
        if(ymd1!=currentYmd){
            $("body #newCalendar2 .badge-box").removeClass('badge-active');
            $(this).find('span').addClass('badge-active');
        }else{
            $("body #newCalendar2 .badge-box").removeClass('badge-active');
        }

    }
    if(typeStr=='out'){
      clickMonth = month;
      clickDay = day;
      lastExactDate = exactDate;
    }
    // 加载右侧列表
    $("#exactDate").html(exactDate);

    // 渲染事件
    let list = getListEventByKey(ymd1);
    loadEventList(list);
  });

  // 被禁用的日期点击事件(仅限下一月)
  $("body").on('click','.badge-alive-td',function () {
    let day = $(this).find('span').attr('data-day');
    let month = $(this).find('span').attr('data-month');
    let type = $(this).find('span').attr('data-type');
    let typeStr = $(this).find('span').attr('data-type-str');
    let exactDate = $(this).find('span').attr('data-date');
    let ymd1 = $(this).find('span').attr('data-ymd');
    // 判断type是否是14天的日期
    $("body #newCalendar2 .badge-box").removeClass('badge-active');
    $(this).find('span').addClass('badge-active');
    loadMonth(type,day,month,'inner');

    if(typeStr=='out'){
      clickType = type;
      clickMonth = month;
      clickDay = day;
      lastExactDate = exactDate;
    }

    disabledBtn(type);
    // 加载右侧列表
    $("#exactDate").html(exactDate);

    // 渲染事件
    let list = getListEventByKey(ymd1);
    loadEventList(list);
  });

  // 禁用选择
  function disabledBtn(type){
    if(type==1){
      $("#preMonth").css('pointer-events','none');
      $("#preMonth").addClass('calendar-disabled');
      $("#nextMonth").css('pointer-events','all');
      $("#nextMonth").removeClass('calendar-disabled');
    }else{
      $("#nextMonth").css('pointer-events','none');
      $("#nextMonth").addClass('calendar-disabled');
      $("#preMonth").css('pointer-events','all');
      $("#preMonth").removeClass('calendar-disabled');
    }
  }

  // 日历月份切换——上一月,默认展示当天的日期
  $("#preMonth").on('click',function () {
    $(this).css('pointer-events','none');
    $("#nextMonth").css('pointer-events','all');
    $(this).addClass('calendar-disabled');
    $("#nextMonth").removeClass('calendar-disabled');

    let month = listCollectionMonth[0].month;
    loadMonth(1,currentMonthObject.day,month,'inner');
    $("#exactDate").html(currentMonthObject.dayEnglish);
    loadEventList(currentMonthObject.listEvent);
  });
  // 日历月份切换——下一月，默认展示当月的1号
  $("#nextMonth").on('click',function () {
    $(this).css('pointer-events','none');
    $(this).addClass('calendar-disabled');
    $("#preMonth").css('pointer-events','all');
    $("#preMonth").removeClass('calendar-disabled');
    let month = listCollectionMonth[1].month;
    loadMonth(2,1,month,'inner');
    $("#exactDate").html(nextMonthObject.dayEnglish);
    loadEventList(nextMonthObject.listEvent);
  });


  // 加载某月的日历 type 0表示14天类的数据 1上一月 2下一月 day几号
  function loadMonth(type,day,month,typeStr) {
    let arr = [];
    if(type==0){
      arr = objCollectionWeek.listDay;
    }else{
      arr = listCollectionMonth[type-1].listDay;
    }
    /*****加载表头*****/
    var titleOfWeek = objCollectionWeek.titleOfWeek;
    let headHtml = '';
    for(var i=0;i<titleOfWeek.length;i++){
      headHtml += `<th class="text-center font-400">${titleOfWeek[i]}</th>`;
    }
    if(type==0){
      $("#newCalendar thead").html(headHtml);
    }else{
      $("#newCalendar2 thead").html(headHtml);
    }
    /*****加载tbody*****/
    let html = '';
    let jWeek = 0;
    for (let j in arr){
      let num = parseInt(arr[j].month)-parseInt(currentMonth);
      if(jWeek%7==0){
        html+='<tr>';
      }
      if(arr[j].timeline==-1||currentYear!=arr[j].year&&arr[j].month!=currentMonth&&num==-10){
        html +=`<td class="text-center"><span class="badge-disabled no-cursor" disabled>${arr[j].day}</span>`;
      }else if(arr[j].timeline==1&&arr[j].month!=month){
        if(num>=0&&num<=1&&type!=0||num<0&&type!=0){
          let type2 = num==0?1:2;
          html +=`<td class="text-center"><div class="badge-alive-td"><span class="badge-disabled badge-alive" data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="${type2}" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
        }else if(num>=0&&num<=1&&type==0||num<0&&type==0){
          html +=`<td class="text-center"><div class="badge-box-td"><span class="badge-box" data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="0" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
        }else{
          html +=`<td class="text-center"><span class="badge-disabled no-cursor" disabled>${arr[j].day}</span>`;
        }

      }
      // 判断type为0的当天日期，type不为0的点击事件日期-选中效果
      else if(type==0&&arr[j].timeline==0||arr[j].ymd==currentYmd){
        if(arr[j].timeline==0&&typeStr=='out'){
          clickDay = arr[j].day;
          clickMonth = arr[j].month;
          lastExactDate = arr[j].dayEnglish;
          currentMonthObject = arr[j];
          ymd = arr[j].ymd;
            currentYmd = ymd;
        }

        html +=`<td class="text-center"><div class="badge-box-td"><span class="badge-box badge-c-active" data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="${type}" data-date="${arr[j].dayEnglish}" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
      }else if(type!=0&&arr[j].timeline==0){
        html +=`<td class="text-center"><div class="badge-alive-td"><span class="badge-box badge-alive" data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="1" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
      }else if(type!=0&&day==arr[j].day){
          html +=`<td class="text-center"><div class="badge-box-td"><span class="badge-box badge-active"  data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="${type}" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
      }
      else{
        html +=`<td class="text-center"><div class="badge-box-td"><span class="badge-box"  data-month="${arr[j].month}" data-day="${arr[j].day}" data-type="${type}" data-date="${arr[j].dayEnglish}" data-ymd="${arr[j].ymd}" data-type-str="${typeStr}">${arr[j].day}</span>`;
      }

      // 活动事件拼接
      let listEvent = arr[j].listEvent;
      if(listEvent.length>0){
        html+='<div class="event-box">';
        for(var k=0;k<listEvent.length;k++){
            let mark = listEvent[k].mark;
          if(k<2){
            if(type==1&&num==1||type==2&&num==0){
              html+=`<a class="badge-tag" style="color:${FONT_CONFIG[mark].fontColor};background: ${FONT_CONFIG[mark].fontColor};opacity: 0.2"></a>`
            }else{
              html+=`<a class="badge-tag" style="color:${FONT_CONFIG[mark].fontColor};background: ${FONT_CONFIG[mark].fontColor}"></a>`
            }

          }
        }
        html+='</div></div></td>';
      }else{
        html+='<div class="event-box"></div></td>';
      }

      // 获取下一月的号的值
      if(type!=0&&arr[j].timeline==1&&arr[j].day==1&&(num==1||num<0)){
        nextMonthObject = arr[j];
      }
      jWeek++;
    }
    if(jWeek%7==0){
      html+='</tr>';
    }
    /*****渲染tbody*****/
    if(type==0){
      $("#newCalendar tbody").html(html);
    }else{
      let currentYear = listCollectionMonth[type-1].monthEnglish+' '+listCollectionMonth[type-1].year;
      $("#newTime2").text(currentYear);
      $("#newCalendar2 tbody").html(html);
    }
  };

  //加载日历后，查询日历的待处理事件
  function LoadMonthAfterGetEvent(){
    $.ajax({
        async:false,
      url: "{{ url('account/buyer_central/getCalendarRemainder') }}",
      type: 'GET',
      dataType: 'json',
      //data: data,
      beforeSend: function () {
      },
      success: function (json) {
        listCollectionMonth = json.data.listCollectionMonth;
      },
      error: function (xhr, ajaxOptions, thrownError,) {
      },
      complete :function(XMLHttpRequest, textStatus){
      }
    });
  }

  // 展示右侧的列表
  function loadEventList(list) {
    if(list.length>0){
      let html = '';
      for(var i=0;i<list.length;i++){
        let startIndex = list[i].description.indexOf('%s');
        let strLeft = list[i].description.substr(0,startIndex);
        let strRight = list[i].description.substr(startIndex+2);
        let mark = list[i].mark;
        html += `<div class="event-panel" style="background: ${FONT_CONFIG[mark].bgColor}"><div class="flex2">`;
        html += `<div class="event-img" style="background: ${FONT_CONFIG[mark].fontBgColor}"><i class="${FONT_CONFIG[mark].fontClass}" style="color:${FONT_CONFIG[mark].fontColor}"></i></div>`;
        html += `<div class="event-list">`;
        html += `<div class="event-list-top">`;
        html += `<span class="float-left font14 bold">${list[i].title}</span>`;
        html += `<span class="float-right f12 font-400 color6">${list[i].time}</span>`;
        html += `</div>`;
        html += `<div class="event-list-bottom both font14 font-400">`;
        html += `${strLeft}<a class="event-link" href="${list[i].url}" target="_blank">${list[i].orderNo}</a>${strRight}`;
        html += `</div></div></div></div>`;
      }
      $("#listEvent").html(html);
      $("#listEvent").show();
      $("#listEvent-none").hide();
    }else{
      $("#listEvent").hide();
      $("#listEvent-none").show();
    }

  };

  // 根据key找到活动事件
  function getListEventByKey(key) {
    var list = [];
    for (var i = 0; i < listCollectionMonth.length; i++) {
      let itemMonth = listCollectionMonth[i];
      if (itemMonth.listDay.hasOwnProperty(key)) {
        return itemMonth.listDay[key].listEvent;
      }
    }
    return list;
  };

  // 打开评分
  $('body').on("click","#viewScoreDetail",function () {
    scoreDailog=layer.open({
      type:1,
      area:['840px','400px'],
      title: '',
      closeBtn: 0,
      content: $('#scoreDetail'),
    })
  });

  // 关闭评分弹窗
  $('body').on("click","#closeScoreDailog",function () {
    layer.close(scoreDailog)
  });

  $(document).ready(function () {
    let information = $(".information").outerHeight();
    let Pending = $(".Pending").outerHeight();
    let title = $(".salesRanking .tab-title").outerHeight();
    let head = $(".salesRanking .tab-head").outerHeight();
    let unread = $(".unread").outerHeight();
    let promotion = $(".promotion").outerHeight();
    if (promotion == null || promotion == undefined || promotion == " ") {
      let salesRanking = parseInt(unread) - parseInt(Pending)- parseInt(32);
      $(".salesRanking ").height(salesRanking);
      let hi = parseInt(information) + parseInt(unread) + parseInt(10);
      $(".guide").height(hi)
      let body =  parseInt(salesRanking) - parseInt(title)- parseInt(head )- parseInt(10);
      $(".salesRanking  .tab-body").height(body);
    } else {
      let salesRanking = parseInt(promotion) + parseInt(unread) - parseInt(Pending)- parseInt(22);
      $(".salesRanking ").height(salesRanking);
      let body =  parseInt(salesRanking) - parseInt(title)- parseInt(head )- parseInt(10);
      $(".salesRanking  .tab-body").height(body);
      let hi = parseInt(information) +  parseInt(promotion) + parseInt(unread) + parseInt(20);
      $(".guide").height(hi)
    }

  })
</script>
{{ footer }}
