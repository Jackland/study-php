<style>
  .form-group label{
    text-transform: none;
  }
  .nav-tabs{
    border: 0;
  }
  .nav-tabs .tip-num {
    text-align: center;
    display: inline-block;
    border-radius: 20px;
    font-style: normal;
    position: absolute;
    padding: 2px 8px;
    bottom: 26px;
    right: -2px;
    background: #fa6400;
    border: 1px solid #fa6400;
    color: #fff;
    font-size: 12px;
    line-height: 16px;
    z-index: 99;
  }
  .tab-pane{
    background: #fff;
    box-shadow: #aaa 0 0 6px;
    border-radius: 3px;
  }
  .mrt30{
    margin-top: 30px;
  }
  .flex-start{
    display: flex;
    justify-content: flex-start;
    align-items: center;
  }
  .flex-around{
    display: flex;
    justify-content: space-around;
    align-items: center;
  }
  .flex-between{
    display: flex;
    justify-content: space-between;
    align-items: center;
  }

  /*规则的css*/
  .freight-sub-nav-tab{
