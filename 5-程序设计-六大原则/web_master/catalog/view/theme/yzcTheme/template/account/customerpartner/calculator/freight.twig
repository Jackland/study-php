      app.qtyError = "Required";
      if (Number(qty) < qtyMin) {
        app.qtyError = `Please enter a number greater than 0`;
      }
      return false;
    } else {
      app.qtyError = "";
    }
    if (day.length < 1 || isNaN(Number(day)) || Number(day) < dayMin || Number(day) > dayMax || lengthOfFractionalPar(day) > 0) {
      app.dayError = "Required";
      if (Number(day) < dayMin) {
        app.dayError = `Please enter a number greater than 0`;
      }
      return false;
    } else {
      app.dayError = "";
    }

    let data = {
      "hasItem": hasItem,
      "sku": sku,
      "comboFlag": comboFlag,
      "comboList": comboList,
      "ltlFlag": ltlFlag,
      "dangerFlag": dangerFlag,
      "length": length,
      "width": width,
      "height": height,
      "weight": weight,
      "qty": qty,
      "day": day
    };
    freightClearResults();
    $.ajax({
      url: "{{ url('account/customerpartner/calculator/freightDo') }}",
      type: 'POST',
      contentType: "application/json;charset=UTF-8",
      dataType: 'json',
      data: JSON.stringify(data),
      beforeSend: function () {
        layer.load();
      },
      success: function (json) {
        if(json.code==200){
          if(json.data.ltlFlag == true){
            json.data.dropShip.expressFreight="--";
            json.data.dropShip.expressFreightPure="--";
          }else {
            json.data.dropShip.ltlFreight="--";
            json.data.dropShip.ltlFreightPure="--";
          }
          $("#dsExpressFreight").text(json.data.dropShip.expressFreight);
          $("#dsExpressFreightPure").text(json.data.dropShip.expressFreightPure);
          $("#dsLtlFreight").text(json.data.dropShip.ltlFreight);
          $("#dsLtlFreightPure").text(json.data.dropShip.ltlFreightPure);
          $("#dsPackageFee").text(json.data.dropShip.packageFee);
          $("#dsPackageFeePure").text(json.data.dropShip.packageFeePure);
          $("#dsTotal").text(json.data.dropShip.total);
          $("#dsTotalPure").text(json.data.dropShip.totalPure);
          $("#puPackageFee").text(json.data.pickUp.packageFee);
          $("#puPackageFeePure").text(json.data.pickUp.packageFeePure);
          $("#puTotal").text(json.data.pickUp.total);
          $("#puTotalPure").text(json.data.pickUp.totalPure);
          $("#wrVolume").text(json.data.wareHouseRental.volume + "ft³");
          let trStr = "";
          for(let index in json.data.wareHouseRental.feeList){
            let item = json.data.wareHouseRental.feeList[index];
            trStr += ('<tr>' +
              '<td class="w240 text-center pad-small">' + $.trim(volumeDayList[index]) + '</td>' +
              '<td class="w480 pad-small text-center color-org font-weight">' + $.trim(volumeQuoteList[index]) + '</td>' +
              '<td class="w240 text-center pad-small color-org font-weight">' + item + '</td>' +
              '</tr>');
          }
          trStr += ('<tr>'+
            '<td class="w240 text-center pad-small">' + $.trim(volumeDayList[volumeDayList.length-1]) + '</td>' +
            '<td class="w480 pad-small text-center color-org font-weight">--</td>' +
            '<td class="w240 text-center pad-small color-org font-weight">' + json.data.wareHouseRental.feeTotal + '</td>' +
            '</tr>');
          $("#wrList").html(trStr);
        } else if (json.code == 404000){
          layer.alert(json.msg, {
            title: 'Message',
            btn : 'OK',
            skin: 'yzc_layer',
            btnAlign: 'c',
          });
          freightClearSkuNotExist();
        } else {
          layer.alert(json.msg, {
            title: 'Message',
            btn : 'OK',
            skin: 'yzc_layer',
            btnAlign: 'c',
          });
        }
      },
      error: function (xhr, ajaxOptions, thrownError,) {

      },
      complete: function(xhr, textStatus){
        layer.closeAll('loading');
      }
    })
  }

