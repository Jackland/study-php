  /**
   * 整数部分的长度
   * @param num  string|number
   * @returns {number} int
   */
  function lengthOfIntegerPar(num) {
    num = Number(num);
    if (isNaN(num)) {
      return 0;
    }

    num = Number(num).toString();
    let point = num.indexOf(".");
    if (point === -1) {
      return num.toString().length;
    }

    return num.toString().substr(0, point).length;
  }

  /**
   * 小数部分的长度
   * @param num  string|number
   * @returns {number} int
   */
  function lengthOfFractionalPar(num) {
    num = Number(num);
    if (isNaN(num)) {
      return 0;
    }

    num = Number(num).toString();
    let point = num.indexOf(".");
    if (point === -1) {
      return 0;
    }

    return num.substr(point + 1).length;
  }
