    border-bottom: 1px solid #e3e3e3 ;
  }

  .platform-rules {
    display: none;
  }
  .platform-rules .table-body{
    max-height: 400px;
    overflow: auto;
  }
  .platform-rules p{
    word-break: break-word;
  }
  .w240{
    width: 240px;
  }
  .w480{
    width: 480px;
  }
  .color-org{
    color: #fA6400 !important;
  }
  #thatForm .bc-white{
    background-color: #FFFFFF !important;
  }
  #thatForm .bc-lightgray{
    background-color: #f3f5f7 !important;
  }
  .font-weight{
    font-weight: bolder;
  }
  .font-tip-small{
    cursor: pointer;
    font-size: 14px;
  }
  .mr10{
    margin-right: 10px !important;
  }
  .pt9{
    padding-top: 9px;
  }
  .border-radius-none{
    border-radius: 4px 0 0 4px !important;
  }
  .unit-tip{
    width: 40px;
    display: inline-block;
  }
  .btn.btn-primary.new-primary{
    background: #3A75DC;
    border-color: #3A75DC;
  }
  .btn.btn-primary.new-primary:hover{
    background: #2d57a9;
    border-color: #2d57a9;
  }
  .btn.btn-default.new-default:hover{
    background: #3A75DC;
    border-color: #3A75DC;
    color: #fff;
  }
  #ele-item-combo{
    margin: 25px 0;
  }
  .combo-list-row{
    margin-bottom: 10px;
  }
  .w15{
    width: 15%;
  }
  .w10{
    width: 10%;
  }
  .width320{
    width: 480px;
  }
  .control-label.myLabel{
    width: 160px;
  }
  .remove-btn:hover,.add-btn:hover{
    cursor: pointer;
    color: #3A75DC;
  }
  .btn-con{
    width: 480px;
    margin-left: 160px !important;
  }
  .action-btn i{
    margin-right: 8px;
  }
  .action-btn i:last-child{
    margin-right: 0;
  }
  .action-btn i:first-child{
    margin-left: 8px;
  }
  .mr2{
    margin-right: 1%;
  }
  .form-control.input-error{
    border-color: #ff414d;
  }
  .error-text{
    color: #ff414d;
    position: absolute;
  }
  #content-calculator .form-group{
    padding-bottom: 16px;
  }
  #content-calculator .required::after{
    content: "*";
    color: '';
  }
  .index-subsku{
    margin-left: 2px;
  }
  [v-cloak] {
    display: none;
  }
