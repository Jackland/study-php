  /**
   * @param numString  string
   * @returns string
   */
  function checkDecimalProcess(numString) {
    let intLength = lengthOfIntegerPar(inchesMax);
    let objArr = numString.match(/^\d*(\.?\d{0,2})/g);
    if (objArr === null) {
      return "";
    }
    let numStrNew = objArr[0];
    if (lengthOfIntegerPar(numStrNew) > intLength) {
      numStrNew = numStrNew.substr(0, intLength);
    }
    return numStrNew;
  }

  /**
   * @param numString  string
   * @returns string
   */
  function checkWeightProcess(numString) {
    let intLength = lengthOfIntegerPar(weightMax);
    let objArr = numString.match(/^\d*(\.?\d{0,2})/g);
    if (objArr === null) {
      return "";
    }
    let numStrNew = objArr[0];
    if (lengthOfIntegerPar(numStrNew) > intLength) {
      numStrNew = numStrNew.substr(0, intLength);
    }
    return numStrNew;
  }

  /**
   * @param numString  string
   * @returns string
   */
  function checkIntegerProcess(numString) {
    let objArr = numString.match(/^\d*/g);
    if (objArr === null) {
      return "";
    }
    let numStrNew = objArr[0];
    if (lengthOfIntegerPar(numStrNew) > 4) {
      numStrNew = numStrNew.substr(0, 4);
    }
    if (Number(numStrNew) == 0) {
      return "";
    }
    return Number(numStrNew).toString();
  }


