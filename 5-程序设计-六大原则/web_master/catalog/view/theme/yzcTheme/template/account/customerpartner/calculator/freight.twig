</style>
<div class="container-fluid" id="content" style="margin-left: 235px">
  <div class="row">
    <div class="col-sm-12 text-center">
      <h1 class="mrt30" data-zh="大健云仓费用计算器">Fulfillment & Storage Calculator</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">

      <ul class="nav nav-tabs">
        <li id="calculator_li" class="active">
          <a href="#tab-calculator" data-toggle="tab" data-zh="费用计算器">Fee Calculation</a>
        </li>
        <li id="rule_li">
          <a href="#tab-rule" data-toggle="tab" data-zh="计费规则">Fees Policy</a>
        </li>
      </ul>
      <div class="tab-content">
        <div class="tab-pane active" id="tab-calculator">
          <fieldset>
            <div id="content-calculator" class="well">
              <div class="row" id="app">
                <div class="col-sm-6">
                  <form id="thatForm" class="form-horizontal calc-width">

                    <div class="form-group">
                      <label for="hasItem" class="col-sm-3 control-label myLabel" data-zh="Item是否存在">If an Item Exists</label>
                      <div class="col-sm-9 width320">
                        <div class="col-sm-6 pt9">
                          <input autocomplete="off" type="radio" name="hasItem" class="mr10" value="1" data-zh="已有Item" v-model="hasItem" v-on:change="checkItemExist">Existing Item
                        </div>
                        <div class="col-sm-6 pt9">
                          <input autocomplete="off" type="radio" name="hasItem" class="mr10" value="0" data-zh="新Item" v-model="hasItem" v-on:change="checkItemExist">New Item
                        </div>
                      </div>
                    </div>
                    <div class="form-group" id="eleItemCode" v-show="hasItem == '-1' || hasItem == '1'">
                      <label for="iptItemCode" class="col-sm-3 control-label myLabel">Item  Code</label>
                      <div class="col-sm-9 width320">
                        <input autocomplete="off" disabled type="text" class="form-control" id="iptItemCode" name="iptItemCode"
                               v-model="iptItemCode"
                               v-bind:class="{'bc-lightgray': hasItem != '1', 'bc-white': hasItem == '1'}"
                               v-bind:disabled="hasItem == '-1' || hasItem == '0'"
                        >
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="itemType" class="col-sm-3 control-label myLabel">Item  Type</label>
                      <div class="col-sm-9 width320">
                        <div class="col-sm-6 pt9">
                          <input autocomplete="off" type="radio" name="itemType" class="mr10" value="0" v-model="itemType" v-bind:disabled="hasItem==-'1' || hasItem=='1'">General Item
                        </div>
                        <div class="col-sm-6 pt9">
                          <input autocomplete="off" type="radio" name="itemType" class="mr10" value="1" v-model="itemType" v-bind:disabled="hasItem=='-1' || hasItem=='1'">Combo Item
                        </div>
                      </div>
                    </div>

                    <div id="ele-item-general" v-show="itemType=='-1' || itemType=='0'">
                      <div class="form-group">
                        <label class="col-sm-3 control-label myLabel">Length</label>
                        <div class="col-sm-9 width320">
                          <div class="input-group">
                            <input autocomplete="off" type="text" class="form-control border-radius-none" name="length" placeholder="0.01~999.99" step="0.01" min="0.01" max="999.99" maxlength="7"
                                   v-model="length"
                                   v-bind:class="{'bc-lightgray': hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1'), 'bc-white': hasItem == '0', 'input-error':lengthError.length>0}"
                                   v-bind:disabled="hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1')"
                                   v-on:keyup="checkDecimal('length')"
                                   v-on:change="checkDecimal('length')"
                            >
                            <div class="input-group-addon"><span class="unit-tip">inches</span></div>
                          </div>
                          <p class="error-text" v-show="lengthError.length>0" v-cloak>${lengthError}$</p>
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="col-sm-3 control-label myLabel">Width</label>
                        <div class="col-sm-9 width320">
                          <div class="input-group">
                            <input autocomplete="off" type="text" class="form-control border-radius-none" name="width" placeholder="0.01~999.99" step="0.01" min="0.01" max="999.99" maxlength="7"
                                   v-model="width"
                                   v-bind:class="{'bc-lightgray': hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1'), 'bc-white': hasItem == '0', 'input-error':widthError.length>0}"
                                   v-bind:disabled="hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1')"
                                   v-on:keyup="checkDecimal('width')"
                                   v-on:change="checkDecimal('width')"
                            >
                            <div class="input-group-addon"><span class="unit-tip">inches</span></div>
                          </div>
                          <p class="error-text" v-show="widthError.length>0" v-cloak>${widthError}$</p>
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="col-sm-3 control-label myLabel">Height</label>
                        <div class="col-sm-9 width320">
                          <div class="input-group">
                            <input autocomplete="off" type="text" class="form-control border-radius-none" name="height" placeholder="0.01~999.99" step="0.01" min="0.01" max="999.99" maxlength="7"
                                   v-model="height"
                                   v-bind:class="{'bc-lightgray': hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1'), 'bc-white': hasItem == '0', 'input-error':heightError.length>0}"
                                   v-bind:disabled="hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1')"
                                   v-on:keyup="checkDecimal('height')"
                                   v-on:change="checkDecimal('height')"
                            >
                            <div class="input-group-addon"><span class="unit-tip">inches</span></div>
                          </div>
                          <p class="error-text" v-show="heightError.length>0" v-cloak>${heightError}$</p>
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="col-sm-3 control-label myLabel">Weight</label>
                        <div class="col-sm-9 width320">
                          <div class="input-group">
                            <input autocomplete="off" type="text" class="form-control border-radius-none" name="weight" placeholder="0.01~9999.99" step="0.01" min="0.01" max="9999.99" maxlength="7"
                                   v-model="weight"
                                   v-bind:class="{'bc-lightgray': hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1'), 'bc-white': hasItem == '0', 'input-error':weightError.length>0}"
                                   v-bind:disabled="hasItem == '-1' || hasItem == '1' || (hasItem == '0' && itemType == '-1')"
                                   v-on:keyup="checkWeight('weight')"
                                   v-on:change="checkWeight('weight')"
                            >
                            <div class="input-group-addon"><span class="unit-tip">lbs</span></div>
                          </div>
                          <p class="error-text" v-show="weightError.length>0" v-cloak>${weightError}$</p>
                        </div>
                      </div>
                    </div>
                    <div id="ele-item-combo" class="form-group" v-show="itemType=='1'" v-cloak>
                      <div class="combo-list-header flex-start">
                        <div class="w15 mr2"></div>
                        <div class="w15 mr2" v-show="hasItem=='1'"></div>
                        <div class="w10 mr2 text-center">Length <br> (inches)</div>
                        <div class="w10 mr2 text-center">Width <br> (inches)</div>
                        <div class="w10 mr2 text-center">Height <br> (inches)</div>
                        <div class="w10 mr2 text-center">Weight  <br> (lbs)</div>
                        <div class="w10 mr2 text-center">Quantity</div>
                        <div class="w10 mr2 text-center"></div>
                      </div>
                      <div v-for="(item, key) in comboList">
                        <div class="combo-list-row flex-start">
                          <div class="w15 mr2 text-center">Sub-item<span class="index-subsku" v-model="key" v-cloak>${key+1}$</span></div>
                          <div class="w15 mr2 text-center name-subsku" v-show="hasItem=='1'" v-cloak>${item.sku}$</div>
                          <div class="w10 mr2 text-center">
                            <input autocomplete="off" type="text" name="length" step="0.01" min="0.01" max="999.99" class="form-control text-center" maxlength="7"
                                   v-model="item.length"
                                   v-bind:class="{'bc-lightgray':hasItem!='0', 'bc-white':hasItem=='0', 'input-error':item.lengthError.length>0}"
                                   v-bind:disabled="hasItem=='1'"
                                   v-on:keyup="checkDecimal('length', key)"
                                   v-on:change="checkDecimal('length', key)"
                            >
                          </div>
                          <div class="w10 mr2 text-center">
                            <input autocomplete="off" type="text" name="width" step="0.01" min="0.01" max="999.99" class="form-control text-center" maxlength="7"
                                   v-model="item.width"
                                   v-bind:class="{'bc-lightgray':hasItem!='0', 'bc-white':hasItem=='0', 'input-error':item.widthError.length>0}"
                                   v-bind:disabled="hasItem=='1'"
                                   v-on:keyup="checkDecimal('width', key)"
                                   v-on:change="checkDecimal('width', key)"
                            >
                          </div>
                          <div class="w10 mr2 text-center">
                            <input autocomplete="off" type="text" name="height" step="0.01" min="0.01" max="999.99" class="form-control text-center" maxlength="7"
                                   v-model="item.height"
                                   v-bind:class="{'bc-lightgray':hasItem!='0', 'bc-white':hasItem=='0', 'input-error':item.heightError.length>0}"
                                   v-bind:disabled="hasItem=='1'"
                                   v-on:keyup="checkDecimal('height', key)"
                                   v-on:change="checkDecimal('height', key)"
                            >
                          </div>
                          <div class="w10 mr2 text-center">
                            <input autocomplete="off" type="text" name="weight" step="0.01" min="0.01" max="999.99" class="form-control text-center" maxlength="7"
                                   v-model="item.weight"
                                   v-bind:class="{'bc-lightgray':hasItem!='0', 'bc-white':hasItem=='0', 'input-error':item.weightError.length>0}"
                                   v-bind:disabled="hasItem=='1'"
                                   v-on:keyup="checkWeight('weight', key)"
                                   v-on:change="checkWeight('weight', key)"
                            >
                          </div>
                          <div class="w10 mr2 text-center">
                            <input autocomplete="off" type="text" name="qty" step="1" min="1" max="9999" class="form-control text-center" maxlength="4"
                                   v-model="item.qty"
                                   v-bind:class="{'bc-lightgray':hasItem!='0', 'bc-white':hasItem=='0', 'input-error':item.qtyError.length>0}"
                                   v-bind:disabled="hasItem=='1'"
                                   v-on:keyup="checkInteger('qty', key)"
                                   v-on:change="checkInteger('qty', key)"
                            >
                          </div>
                          <div class="w10 mr2 action-btn">
                            <i class="giga icon-lajitong remove-btn" v-show="hasItem==0 && key > 0" v-on:click="removeCombo(this, key)"></i>
                            <i class="giga icon-action-add add-btn" v-show="hasItem==0" v-on:click="addCombo"></i>
                          </div>
                        </div>
                        <div class="combo-list-row flex-start">
                          <div class="error-text" style="position: unset; margin-left: 15%;" v-show="item.itemError.length>0" v-cloak>${item.itemError}$</div>
                        </div>
                      </div>

                    </div>

                    <div class="form-group">
                      <label class="col-sm-3 control-label myLabel">Quantity</label>
                      <div class="col-sm-9 width320">
                        <div class="input-group">
                          <input autocomplete="off" type="text" class="form-control border-radius-none bc-white" id="iptQty" name="iptQty" placeholder="1~9999" value="1" step="1" min="1" max="9999"  maxlength="4"
                                 v-model="qty"
                                 v-bind:class="{'input-error':qtyError.length>0}"
                                 v-on:keyup="checkInteger('qty')"
                                 v-on:change="checkInteger('qty')"
                          >
                          <div class="input-group-addon"><span class="unit-tip">pcs</span></div>
                        </div>
                        <p class="error-text" v-show="qtyError.length>0" v-cloak>${qtyError}$</p>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="col-sm-3 control-label myLabel" data-zh="在库天数">Days in Stock</label>
                      <div class="col-sm-9 width320">
                        <div class="input-group">
                          <input autocomplete="off" type="text" class="form-control border-radius-none bc-white" id="iptDay" name="iptDay" placeholder="1~9999" value="1" step="1" min="1" max="9999" maxlength="4"
                                 v-model="day"
                                 v-bind:class="{'input-error':dayError.length>0}"
                                 v-on:keyup="checkInteger('day')"
                                 v-on:change="checkInteger('day')"
                          >
                          <div class="input-group-addon"><span class="unit-tip">Days</span></div>
                        </div>
                        <p class="error-text" v-show="dayError.length>0" v-cloak>${dayError}$</p>
                      </div>
                    </div>

                    <div class="form-group text-center btn-con">
                      <button type="button" class="btn btn-primary new-primary" onclick="freightDo()" v-bind:disabled="hasItem=='-1' || itemType=='-1'">CALCULATE</button>
                      <button type="reset" class="btn btn-default new-default" onclick="freightClear()">RESET</button>
                    </div>
                  </form>
                </div>
                <div class="col-sm-6">
                  <p class="free-detail-title font-weight" data-zh="物流费用明细">Fulfillment Fee Detail：<i class="giga icon-V10-wenhaotishi font-tip-small" data-toggle="tooltip" title="Fulfillment Fee = Shipping Fee + Packing Fee" data-zh="物流费用=运费+打包费，上门取货类型只有打包费"></i> </p>
                  <div class="one-send">
                    <div class="tab-sub-table-title" data-zh="一件代发">Drop Shipping：</div>
                    <div class="table-header">
                      <table class="table title-table">
                        <tr><th class="w240 text-center pad-small" data-zh="费用项">Fee Type</th><th class="w480 text-center pad-small" data-zh="平台云仓交易">B2B Marketplace</th><th class="w240 text-center pad-small" data-zh="第三方物流">3rd Party Logistics</th></tr>
                      </table>
                    </div>
                    <div class="table-body">
                      <table class="table table-bordered">
                        <tr>
                          <td class="w240 text-center pad-small" data-zh="快递运费">Parcel Carrier Fee({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 text-center pad-small color-org font-weight" id="dsExpressFreight"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="dsExpressFreightPure"></td>
                        </tr>
                        <tr>
                          <td class="w240 text-center pad-small" data-zh="LTL运费">LTL Freight Fee({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 pad-small text-center color-org font-weight" id="dsLtlFreight"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="dsLtlFreightPure"></td>
                        </tr>
                        <tr>
                          <td class="w240 text-center pad-small" data-zh="打包费">Packing Fee({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 pad-small text-center color-org font-weight" id="dsPackageFee"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="dsPackageFeePure"></td>
                        </tr>
                        <tr>
                          <td class="w240 text-center pad-small font-weight">Total({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 pad-small text-center color-org font-weight" id="dsTotal"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="dsTotalPure"></td>
                        </tr>
                      </table>
                    </div>
                  </div>
                  <div class="home-send">
                    <div class="tab-sub-table-title" data-zh="上门取货">Pickup：</div>
                    <div class="table-header">
                      <table class="table title-table">
                        <tr><th class="w240 text-center pad-small" data-zh="费用项">Fee Type</th><th class="w480 text-center pad-small" data-zh="平台云仓交易">B2B Marketplace</th><th class="w240 text-center pad-small" data-zh="第三方物流">3rd Party Logistics</th></tr>
                      </table>
                    </div>
                    <div class="table-body">
                      <table class="table table-bordered">
                        <tr>
                          <td class="w240 text-center pad-small" data-zh="打包费">Packing Fee({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 pad-small text-center color-org font-weight" id="puPackageFee"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="puPackageFeePure"></td>
                        </tr>
                        <tr>
                          <td class="w240 text-center pad-small font-weight">Total({{ symbolLeft }}{{ symbolRight }})</td>
                          <td class="w480 pad-small text-center color-org font-weight" id="puTotal"></td>
                          <td class="w240 text-center pad-small color-org font-weight" id="puTotalPure"></td>
                        </tr>
                      </table>
                    </div>
                  </div>
                  <br>
                  <p class="free-detail-title font-weight" data-zh="仓租费明细">Storage Fee Detail</p>
                  <div class="one-send">
                    <div class="tab-sub-table-title">Volume：<span class="font-weight color-org" id="wrVolume"></span></div>
                    <div class="table-header">
                      <table class="table title-table">
                        <tr><th class="w240 text-center pad-small" data-zh="费用项">Fee Type</th><th class="w480 text-center pad-small" data-zh="适用阶梯报价">Applicable Tiered Quote</th><th class="w240 text-center pad-small" data-zh="费用结果">Result</th></tr>
                      </table>
                    </div>
                    <div class="table-body">
                      <table class="table table-bordered">
                        <tbody id="wrList">
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-12">
                  <br>
                  <br>
