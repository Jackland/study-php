  function getUploadFileByOrder(wfu) {
    var file_str = $('#file_str').val();
    var id = wfu.uploadId;
    var file_name = wfu.fileList[0].name;
    var file_size = wfu.fileList[0].size;
    if (file_size == 0) {
      var size_err = '{{ error_file_size }}';
      layerMsg(size_err);
      return false;
    }
    var edit_flag = '{{ edit_first }}';
    // is_special 1 为全不校验， 0 为不校验
    if (is_special == 0) {
      if (edit_flag == 1) {
        file_str += id + ':' + file_name + ';';
        var order_id = id.split('_')[0];
        var file_name1 = [];
        var file_arr = file_str.split(';');
        $.each(file_arr, function (idx, obj) {
          if (obj != null && obj != '') {
            var i = obj.split(':');
            file_name1.push(i[1]);
          }
        });
        file_name_unique = [...new
        Set(file_name1)];
        if (file_name1.length != file_name_unique.length) {
          layerMsg('{{ error_file_repeated }}');
          $('#' + wfu.uploadId + ' .fileItem').hide();
          return false;
        }
      } else {
        var tmp = '';
        $.each(json.container_id_list.id_list, function (idx, obj) {
          if (walmart_common_label[obj] != null && walmart_common_label[obj] != '') {
            tmp += walmart_common_label[obj] + ',';
          }
        });
        tmp += wfu.fileList[0].name;
        var verify_file_list = tmp.split(',');
        var verify_file_list_unique = [...new
        Set(verify_file_list)
      ]
        ;
        if (verify_file_list.length != verify_file_list_unique.length) {
          layerMsg('{{ error_file_repeated }}');
          $('#' + wfu.uploadId + ' .fileItem').hide();
          return false;
        }
      }
    }

    return true;
  }
  function getUploadBolByOrder(wfu) {
    var file_name = wfu.fileList[0].name;
    var file_size = wfu.fileList[0].size;
    if (file_size == 0) {
      var size_err = '{{ error_file_size }}';
      layerMsg(size_err);
      return false;
    }
    var ret = 1;
    $.each(json.walmart_bol, function (idx, obj) {
      if (file_name == walmart_bol_list[obj]) {
        layerMsg('{{ error_bol_repeated }}');
        $('#' + wfu.uploadId + ' .fileItem').hide();
        ret = 0;
        return false;
      }
    });
    if(ret == 0){
      return  false;
    }

    //判断是否可以check
    return true;
  }
