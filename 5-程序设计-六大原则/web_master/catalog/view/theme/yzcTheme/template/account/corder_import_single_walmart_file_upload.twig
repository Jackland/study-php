        parent.layer.close(index);
      },
      end: function () {

      }
    });
  }
  //更改bol状态
  function changeBolStatus(obj) {
    var confirm_tips = 'Are you sure to delete this bol?';
    parent.layer.confirm(confirm_tips, {
      title: 'Message',
      skin: 'yzc_layer', //样式类名
      btn: ['Yes', 'No'],
      yes: function (index) {
        $(obj).parent().hide();
        $(obj).parent().prev().show();
        var bol_id = $(obj).attr('id').split('_')[0] + '_bol';
        walmart_bol_list[bol_id] = '';
        calcOrderInfoByEdit(bol_id);
        parent.layer.close(index);
      },
      end: function () {
      }
    });
  }

