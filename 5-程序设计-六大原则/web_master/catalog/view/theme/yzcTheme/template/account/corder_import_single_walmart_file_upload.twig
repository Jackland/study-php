  function calcOrderInfo(uploadId) {
    //多少个订单被选中
    //已上传的文件个数
    //订单是否需要被选中
    var file_str = $('#file_str').val();
    var order_id = uploadId.split('_')[0];
    count = 0;
    // label上传的部分
    var file_arr = file_str.split(';');
    $.each(file_arr, function (idx, obj) {
      if (obj != null && obj != '') {
        var i = obj.split(':');
        if (i[0].indexOf(order_id) == 0) {
          count++;
        }
      }
    });
    var num = json['container_id_list'][order_id]['count'];
    // 计算bol部分
    var bol_id = order_id + '_bol';
    if (walmart_bol_list[bol_id]) {
      count++;
    }
    // 计算store_label部分
    if (json.walmart_store_label) {
      $.each(json.walmart_store_label, function (idx, obj) {
        if (walmart_store_label[obj] != null && walmart_store_label[obj] != '') {
          if (obj.indexOf(order_id) == 0) {
            count++;
          }
        }
      });
    }
    // 更改显示数量
    var edit_flag = '{{ edit_first }}';
    if (edit_flag == 1) {
      var order_span = '#' + order_id + '_span';
      var span_str = '(' + count + '/' + num + ')';
      $(order_span).html(span_str);
    }
  }
  //获取重复的pdf的名称
  function duplicates(arr) {
    var result = [];
    for (var i = 0; i < arr.length; i++) {
      if ($.inArray(arr[i], result) == -1) {
        result.push(arr[i]);
      } else {
        return arr[i];
      }
    }


    return result;
  }
  function verifyTrackingNumber(uploadId) {
    var tmp = [];
    var order_id = uploadId.split('_')[0];
    $.each(json.container_id_list.id_list, function (idx, obj) {
      if (walmart_tracking_number[obj] != null && walmart_tracking_number[obj] != '') {
        if (obj.indexOf(order_id) == 0) {
          tmp.push(walmart_tracking_number[obj]);
        }
      }
    });
    var ret = duplicates(tmp);
    if ($.isArray(ret)) {
      return false;

    }
    return ret;

  }
  //关闭弹窗
  function cancelAction() {
    var index = parent.layer.getFrameIndex(window.name);
    parent.layer.close(index);
  }
  function pushTrackingNumber(obj) {
    if (is_special == 0) {
      var input_id = $(obj).attr('id');
      input_id = input_id.replace('_tracking_number', '');
      walmart_tracking_number[input_id] = $.trim($(obj).val());
      var tmp = [];
      var order_id = input_id.split('_')[0];
      $.each(json.container_id_list.id_list, function (idx, obj) {
        if (walmart_tracking_number[obj] != null && walmart_tracking_number[obj] != '') {
          if (obj.indexOf(order_id) == 0) {
            tmp.push(walmart_tracking_number[obj]);
          }
        }
      });
      var ret = duplicates(tmp);
      if (!$.isArray(ret)) {
        layerMsg('The selected orders contain repeat tracking numbers [' + ret + ']', 3000);
      }
    }
  }
  function checkInputInfo() {
    var edit_flag = '{{ edit_first }}';
    if (edit_flag == 1) {
      var file_str = $('#file_str').val();
    }else{
      var file_str = '';
      for(let ks  in walmart_common_label){
        if(walmart_common_label[ks]){
          file_str += ks + ':' + walmart_common_label[ks] + ';'
        }
      }
      file_str = file_str.substring(0,file_str.lastIndexOf(';'));
      $('#file_str').val(file_str);
    }
    $('#btnClose').attr('disabled', 'disabled');
    var file_name = [];

    var order_id = $('#order_id').val();
    //增加一个验证是否重复的
    if (is_special == 0) {
      var file_arr = file_str.split(';');
      $.each(file_arr, function (idx, obj) {
        if (obj != null && obj != '') {
          var i = obj.split(':');
          file_name.push(i[1]);
        }
      });
      file_name_unique = [...new
      Set(file_name)
      ];
      if (file_name.length != file_name_unique.length) {
        var repeat_label = duplicates(file_name);
        layerMsg('The label [' + repeat_label + '] uploaded is repeat with the other label,please upload again.');
        $('#btnClose').attr('disabled', false);
        return false;
      }
      // 验证tracking是否重复 是否填写完整
      var tmp = [];
      $.each(json.container_id_list.id_list, function (idx, obj) {
        if (walmart_tracking_number[obj] == null || walmart_tracking_number[obj] == '') {
          layerMsg('{{ error_tracking_number_fill }}', 3000);
          submit_flag = 0;
          return false;
        }
        if (walmart_tracking_number[obj] != null && walmart_tracking_number[obj] != '') {
          if (obj.indexOf(order_id) == 0) {
            tmp.push(walmart_tracking_number[obj]);
          }
        }
      });
      if (submit_flag == 0) {
        $('#btnClose').attr('disabled', false);
        submit_flag = 1;
        return false;
      }
      var ret = duplicates(tmp);
      if (!$.isArray(ret)) {
        layerMsg('The selected orders contain repeat tracking numbers [' + ret + ']', 3000);
        $('#btnClose').attr('disabled', false);
        return false;
      }
      // 后端需要验证数据的正确性
      $.ajax({
        url: '{{ walmart_verify_tracking }}',
        type: 'post',
        data: {'tracking_number_list': tmp},
        async: false,
        beforeSend: function () {
        },
        success: function (json) {
          if (json.error == 1) {
            var tracking_str = json.result.join(', ');
            var tracking_error = 'The selected orders contain repeat tracking numbers [' + tracking_str + '], it cannot be submitted.';
            layerMsg(tracking_error, 3000);
            submit_flag = 0;
          }
        }
      });
      if (submit_flag == 0) {
        $('#btnClose').attr('disabled', false);
        submit_flag = 1;
        return false;
      }
    }
    var input_flag = 1;
    var bol_id = order_id + '_bol';
    if ($('#' + bol_id).length > 0) {
      if (walmart_bol_list[order_id + '_bol'] == '') {
        layerMsg('{{ error_bol_fill }}');
        $('#btnClose').attr('disabled', false);
        return false;
      }
      var bol_input_str = order_id + '_bol' + ':' + walmart_bol_list[order_id + '_bol'] + ';';
      //bol 也是要提交的
      $('#bol_str').val(bol_input_str);
    }
    // store_label
    var store_input_str = '';
    if (json.walmart_store_label) {
      $.each(json.walmart_store_label, function (idx, obj) {
        if (walmart_store_label[obj] == null || walmart_store_label[obj] == '') {
          layerMsg('{{ error_file_fill }}');
          $('#btnClose').attr('disabled', false);
          submit_flag = 0;
          return false;
        } else {
          store_input_str += obj + ':' + walmart_store_label[obj] + ';';
        }
      });
      $('#store_input_str').val(store_input_str);
    }

    if (submit_flag == 0) {
      $('#btnClose').attr('disabled', false);
      submit_flag = 1;
      return false;
    }


    if (edit_flag == 1) {
      //首次提交
      var child_list = json['container_id_list'][order_id]['child_list'];
      for (var i = 0; i < child_list.length; i++) {
        if (file_str.indexOf(child_list[i]) == -1) {
          layerMsg('{{ error_file_fill }}');
          $('#btnClose').attr('disabled', false);
          input_flag = 0;
          return false;
        }
      }
    } else {
      //验证walmart_common_label全部为填写状态
      //验证文件是否是重复的
      var tmp = '';
      $.each(json.container_id_list.id_list, function (idx, obj) {
        if (walmart_common_label[obj] == null || walmart_common_label[obj] == '') {
          layerMsg('{{ error_file_fill }}');
          $('#btnClose').attr('disabled', false);
          submit_flag = 0;
          return false;
        }

        if (walmart_common_label[obj] != null && walmart_common_label[obj] != '') {
          tmp += walmart_common_label[obj] + ',';
        }
      });

      if (submit_flag == 0) {
        $('#btnClose').attr('disabled', false);
        submit_flag = 1;
        return false;
      }

      if (is_special == 0) {
        tmp = tmp.substring(0, tmp.length - 1);
        var verify_file_list = tmp.split(',');
        var verify_file_list_unique = [...new
        Set(verify_file_list)
      ]
        ;
        if (verify_file_list.length != verify_file_list_unique.length) {
          layerMsg('{{ error_file_repeated }}');
          $('#' + wfu.uploadId + ' .fileItem').hide();
          return false;
        }
      }
    }
    //ajax 提交
    var btn = $("a.layui-layer-btn0");
    if(btn.hasClass("layui-btn-disabled")){
        return;
    }
    btn.addClass("layui-btn-disabled");
    $.ajax({
      url: '{{ dropship_file_submit }}',
      type: 'post',
      data: $('#form-upload').serializeArray(),
      async: false,
      beforeSend: function () {
        parent.layer.load();
      },
      success: function (json) {
        parent.layer.closeAll('loading');
        layerMsg(json.msg, 2000);
        setTimeout(function () {
          var index = parent.layer.getFrameIndex(window.name);
          parent.layer.close(index);
          btn.removeClass("layui-btn-disabled");
        }, 2 * 1000);
      },
      error: function (xhr, ajaxOptions, thrownError) {
        var index = parent.layer.getFrameIndex(window.name);
        parent.layer.close(index);
        layerMsg(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
        btn.removeClass("layui-btn-disabled");
      }
    });
  }
  function calcOrderInfoByEdit(uploadId) {
    //多少个订单被选中
    //已上传的文件个数
    //订单是否需要被选中
    var order_id = uploadId.split('_')[0];
    count = 0;
    // label上传的部分
    $.each(json.container_id_list.id_list, function (idx, obj) {
      if (walmart_common_label[obj] != null && walmart_common_label[obj] != '') {
        if (obj.indexOf(order_id) == 0) {
          count++;
        }
      }
    });
    var num = json['container_id_list'][order_id]['count'];
    // 计算bol部分
    var bol_id = order_id + '_bol';
    if (walmart_bol_list[bol_id]) {
      count++;
    }
    // 计算store_label部分
    if (json.walmart_store_label) {
      $.each(json.walmart_store_label, function (idx, obj) {
        if (walmart_store_label[obj] != null && walmart_store_label[obj] != '') {
          if (obj.indexOf(order_id) == 0) {
            count++;
          }
        }
      });
    }
    // 更改显示数量
    var order_span = '#' + order_id + '_span';
    var span_str = '(' + count + '/' + num + ')';
    $(order_span).html(span_str);
  }
  // 更改store label状态
  function changeCommonLabelStatus(obj) {
    var common_label_id = $(obj).attr('mean');
    var confirm_tips = 'Are you sure to delete this shipping label?';
    parent.layer.confirm(confirm_tips, {
      title: 'Message',
      skin: 'yzc_layer', //样式类名
      btn: ['Yes', 'No'],
      yes: function (index) {
        walmart_common_label[common_label_id] = '';
        // walmart_tracking_number[common_label_id] = '';
        calcOrderInfoByEdit(common_label_id);
        //展示小图消失，上传控件展示
        $(obj).parent().hide();
        $(obj).parent().prev().empty();
        $(obj).parent().prev().prev().show();
        $('#' + common_label_id + '_show').hide();
        //验证 tracking 是否要被清空，手动填写不需要被清空
        {% if upload_file_info[0]['tracking_number_method'] %}
        $('#' + common_label_id + '_tracking_number').val(null);
        {% endif %}
        parent.layer.close(index);
      },
      end: function () {

      }
    });
  }
