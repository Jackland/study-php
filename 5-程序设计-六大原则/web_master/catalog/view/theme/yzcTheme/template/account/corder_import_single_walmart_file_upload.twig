    //后端删除
    var uploadId = wfu.uploadId;
    var verify = uploadId.split('_')[1];
    if (uploadId.split('_').length == 3) {
      verify = 'store';
    }
    if (verify == 'bol') {
      var id = uploadId + '_show';
