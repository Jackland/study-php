<script>
  $(document).ready(function () {
    // 分数详情的弹框
    var scoreDailog='';
    $('body').on("click","#viewScoreDetail",function () {
      scoreDailog=layer.open({
        type:1,
        area:['840px','400px'],
        title: '',
        closeBtn: 0,
        content: $('#scoreDetail'),
      })
    })
    $('body').on("click","#closeScoreDailog",function () {
      layer.close(scoreDailog)
    })

    // 推荐buyer切换
    var recommendBuyer = {
      _itemEl: $('.recommend-buyer-tab'),
      _onePageCount: 4,
      _nextIndex: 0,
      _totalCount: 0,
      init() {
        this._totalCount = this._itemEl.length
        this.nextPage()
        $('body').on('click', '.recommend-refresh-btn', () => {
          this.nextPage()
        })
      },
      nextPage() {
        this._itemEl.addClass('hidden');
        for (let i = 0; i < this._onePageCount; i++) {
          this._itemEl.eq(this._nextIndex++).removeClass('hidden');
        }
        if (this._nextIndex >= this._totalCount) {
          this._nextIndex = 0;
        }
      }
    }
    recommendBuyer.init();
  })
  function openNoticeForm(url) {
    window.open(url);
  }
</script>
